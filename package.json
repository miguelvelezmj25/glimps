{
  "name": "perf-debug",
  "displayName": "perf-debug",
  "description": "",
  "version": "0.0.1",
  "publisher": "mvelezce",
  "engines": {
    "vscode": "^1.52.0"
  },
  "categories": [
    "Research prototype"
  ],
  "activationEvents": [
    "onCommand:globalModel.start",
    "onCommand:localModels.start"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "globalModel.start",
        "title": "Display global performance model",
        "category": "Perf Debug"
      },
      {
        "command": "localModels.start",
        "title": "Display local performance models",
        "category": "Perf Debug"
      }
    ]
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/miguelvelezmj25/perf-debug.git"
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src --ext ts",
    "test": "node ./out/test/runTest.js"
  },
  "devDependencies": {
    "@types/glob": "7.1.3",
    "@types/mocha": "8.0.4",
    "@types/node": "12.11.7",
    "@types/vscode": "^1.52.0",
    "@typescript-eslint/eslint-plugin": "4.14.1",
    "@typescript-eslint/parser": "4.14.1",
    "eslint": "7.19.0",
    "glob": "7.1.6",
    "mocha": "8.2.1",
    "typescript": "4.1.3",
    "vscode-test": "1.5.1"
  },
  "dependencies": {
    "csv-parse": "4.15.3"
  }
}
